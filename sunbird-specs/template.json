{
			"@context": [
			"https://www.w3.org/2018/credentials/v1",			
			"https://github.com/sknirmalkar89/sunbird-rc-core/blob/main/sunbird-specs/context.json"
		 ],
			"type": [
				"VerifiableCredential"
			],
			"issuanceDate": "{{osCreatedAt}}",
			"credentialSubject": {
        "type":"{{type}}",
				"name": "{{recipient.name}}",
				"trainingName": "{{training.name}}",
				"trainingId":"{{training.id}}"	
			 },
    "signatory":[
				      {{#each signatory}}
				      {
                 
				         "identity":"{{identity}}",
				         "type":[
				            "Extension",
				            "extensions:SignatoryExtension"
				         ],
				         "designation":"{{designation}}",
				         "image":"{{image}}",
				         "name":"{{name}}"
			          }
               {{#unless @last}}, {{/unless}}
				       {{/each}}              
				      
    		],
      "issuer":{			         
                
                 "type":[
                    "Issuer"
                 ],
                 "name":"{{issuer.name}}",
                 "url":"{{issuer.url}}",
                 "publicKey":["{{issuer.kid}}"]
          }
			
     	}
